{
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/cookie-preferences/:path*",
      "destination": "/api/cookie-preferences/:path*"
    }
  ],
  "functions": {
    "api/cookie-preferences/**/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
